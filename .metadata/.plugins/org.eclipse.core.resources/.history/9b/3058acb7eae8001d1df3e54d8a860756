package sec03.exam05;

public class OperationsPromotionExample {

	public static void main(String[] args) {
		
		byte bytevalue1 = 10;
		byte bytevalue2 = 20;
		long result = bytevalue1 + bytevalue2;
		// 말햇듯이 byte변수+byte 변수 더할땐 int, long 같이 상위변수사용
		System.out.println(result);
		
		
		
		
		
		
		
		
	}

}
